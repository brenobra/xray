{
  "name": "site-intelligence",
  "version": "1.0.0",

  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create site-intelligence-db",
    "db:migrate": "wrangler d1 execute site-intelligence-db --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute site-intelligence-db --local --file=./schema.sql"
  },
  "dependencies": {
    "@cloudflare/containers": "^0.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250410.0",
    "typescript": "^5.7.0",
    "wrangler": "^4.14.0"
  }
}
